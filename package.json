{
  "name": "react-magic-zoom",
  "version": "1.3.0",
  "description": "React component for show zoom in image on mouse hover.",
  "main": "./dist/reactMagicZoom.js",
  "scripts": {
    "build": "npm run build-js && webpack && npm run build-css",
    "build-js": "babel src --out-dir dist",
    "build-css": "node-sass ./src/scss/_react-magic-zoom.scss ./dist/css/react-magic-zoom.css",
    "example": "webpack && webpack-dev-server --progress --colors --no-info --content-base example",
    "test": "test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ilkorn/react-magic-zoom.git"
  },
  "keywords": [
    "react",
    "react-magic-zoom",
    "react-component",
    "magic-zoom",
    "zoom-image"
  ],
  "author": "ilkorn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ilkorn/react-magic-zoom/issues"
  },
  "homepage": "https://github.com/Ilkorn/react-magic-zoom#readme",
  "dependencies": {
    "classnames": "^2.2.3",
    "lodash": "^4.13.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-core": "^6.4.5",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^3.2.3",
    "moment": "^2.12.0",
    "node-libs-browser": "^0.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  }
}
